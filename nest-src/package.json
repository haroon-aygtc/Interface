{
  "name": "nest-src",
  "version": "1.0.0",
  "description": "This is a NestJS-based authentication API with user management, role-based access control, and password reset functionality.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:create": "ts-node -r tsconfig-paths/register scripts/create-database.ts",
    "db:migrate": "typeorm migration:run",
    "db:seed": "ts-node -r tsconfig-paths/register scripts/seed-database.ts"
  },
